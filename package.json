{
  "name": "tstutorials",
  "version": "1.0.1",
  "private": true,
  "description": "",
  "type": "module",
  "main": "src/app.ts",
  "scripts": {
    "start:dev:nonRespawn": "ts-node-dev --transpile-only src/app.ts",
    "start:dev": "nodemon src/app.ts",
    "start:build": "tsc && node dist/src/app.js",
    "start": "node --loader ts-node/esm src/app.ts",
    "build": "tsc",
    "deploy": "node src/deploy.js"
  },
  "imports": {
    "#root/*": "./dist/*.js",
    "#src/*": "./dist/src/*.js",
    "#commands/*": "./dist/src/commands/*.js",
    "#configs/*": "./dist/src/configs/*.js",
    "#events/*": "./dist/src/events/*.js",
    "#structures/*": "./dist/src/structures/*.js",
    "#utils/*": "./dist/src/utils/*.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "@discordjs/voice": "^0.9.0",
    "axios": "^0.27.2",
    "discord-api-types": "^0.32.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "ffmpeg-static": "^5.0.0",
    "https-proxy-agent": "^5.0.0",
    "libsodium-wrappers": "^0.7.10",
    "miniget": "^4.2.2",
    "simple-youtube-api": "^5.2.1",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "youtube-dl-exec": "^2.0.10",
    "ytdl-core": "^4.11.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.30",
    "esm": "^3.2.25",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
